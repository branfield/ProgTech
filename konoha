#include<iostream>
using namespace std;

void derevnya(int *mas, int saske)
{
	int k = 1;
	for (int i = 0; i < saske; i++)
	{
		int t = 0;
		for (k = 1; k <= mas[i]; k++)
		{
			if (mas[i] % k == 0)
				t++;
		}
		mas[i] = t;
	}
}

int main()
{
	setlocale(0, "");
	int saske = 0;
	cout << "Введите количество чисел в массиве: ";
	cin >> saske; 
	int *mas = new int[saske];
	for (int i = 0; i < saske; i++)
	{
		cin >> mas[i];
	}
	derevnya(mas, saske);
	for (int i = 0; i < saske; i++)
	{
		cout << mas[i] << " ";
	}

	system("pause");
	return 0;
}
